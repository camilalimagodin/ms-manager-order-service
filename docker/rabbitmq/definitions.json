{
  "rabbit_version": "3.12.0",
  "users": [
    {
      "name": "guest",
      "password_hash": "guest",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": ["administrator"]
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "order.exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "order.dlx",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "order.created.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "order.dlx",
        "x-dead-letter-routing-key": "order.retry.5s"
      }
    },
    {
      "name": "order.retry.queue.5s",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 5000,
        "x-dead-letter-exchange": "order.exchange",
        "x-dead-letter-routing-key": "order.created"
      }
    },
    {
      "name": "order.retry.queue.30s",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 30000,
        "x-dead-letter-exchange": "order.exchange",
        "x-dead-letter-routing-key": "order.created"
      }
    },
    {
      "name": "order.retry.queue.5min",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 300000,
        "x-dead-letter-exchange": "order.exchange",
        "x-dead-letter-routing-key": "order.created"
      }
    },
    {
      "name": "order.created.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "order.parking.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000
      }
    }
  ],
  "bindings": [
    {
      "source": "order.exchange",
      "vhost": "/",
      "destination": "order.created.queue",
      "destination_type": "queue",
      "routing_key": "order.created",
      "arguments": {}
    },
    {
      "source": "order.dlx",
      "vhost": "/",
      "destination": "order.retry.queue.5s",
      "destination_type": "queue",
      "routing_key": "order.retry.5s",
      "arguments": {}
    },
    {
      "source": "order.dlx",
      "vhost": "/",
      "destination": "order.retry.queue.30s",
      "destination_type": "queue",
      "routing_key": "order.retry.30s",
      "arguments": {}
    },
    {
      "source": "order.dlx",
      "vhost": "/",
      "destination": "order.retry.queue.5min",
      "destination_type": "queue",
      "routing_key": "order.retry.5min",
      "arguments": {}
    },
    {
      "source": "order.dlx",
      "vhost": "/",
      "destination": "order.created.dlq",
      "destination_type": "queue",
      "routing_key": "order.dlq",
      "arguments": {}
    }
  ]
}
